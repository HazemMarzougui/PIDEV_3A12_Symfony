<script>
$(document).ready(function(){
    $('#searchInput').on('keyup', function(){
        var searchTerm = $(this).val();
        $.ajax({
            url: "{{ path('app_commande_dash_search') }}",
            type: "GET",
            data: {
                search: searchTerm
            },
            success: function(response){
                var commandes = response.commandes;
                var table = $('#commandesTable');
                table.empty();
                $.each(commandes, function(index, commande){
                    var row = $('<tr>').append(
                        $('<td>').text(commande.prenom),
                        $('<td>').text(commande.nom),
                        $('<td>').text(commande.adresse),
                        $('<td>').text(commande.telephone),
                        $('<td>').text(commande.email),
                        $('<td>').text(commande.prixTotale),
                        $('<td>').append(
                            $('<form>').attr('action', '/dash_commande/' + commande.id + '/delete').attr('method', 'post').append(
                                $('<button>').attr('type', 'submit').addClass('btn btn-danger btn-sm').text('Supprimer')
                            )
                        )
                    );
                    table.append(row);
                });
            }
        });
    });
});
